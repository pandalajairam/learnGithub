/*
Carousel

Wrapper for the slide container and indicators for articles carousel

Markup:
<script>
$(document).ready(function(){
  if( $('.carousel').length > 0 && $('.carousel').hasClass('slide') ) {
    $('.carousel.slide').find(".carousel-inner").swipe( {
      swipeLeft:function(event, direction, distance, duration, fingerCount) {
        $(this).parent().carousel('next');
        $(this).find('.ico-centered').remove();
      },
      swipeRight: function() {
        $(this).parent().carousel('prev');
      },
      threshold:0 //Default is 75px, set to 0 for demo so any distance triggers swipe
    });
  }
  $('#articleSlideshow').on('slid.bs.carousel', function () {
    var active = $(this).find('.active');
    $(this).find('.carousel-index').text($(active).data('index'));
  })
});
</script>
